{
    "name": "minimal-react",
    "version": "1.0.0",
    "description": "Minimal React.js starter kit for experiments.",
    "main": "./src/index.jsx",
    "scripts": {
        "build-client": "webpack --progress",
        "build-server": "babel ./server -d ./build",
        "clean": "rm -rf ./build",
        "start": "npm run build && babel-node server/server.js",
        "client": "webpack --watch",
        "server": "babel-node server/server.js --presets es2015",
        "dev": "concurrently \"npm run server\" \"npm run client\""
    },
    "repository": "https://github.com/BatenkovT/Minimal-React.git",
