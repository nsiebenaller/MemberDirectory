        "babel-plugin-transform-class-properties": "^6.24.1",
        "bluebird": "^3.5.0",
        "bunyan": "^1.8.10",
        "concurrently": "^4.1.0",
        "cookie-parser": "^1.4.4",
        "cors": "^2.8.3",
        "dotenv": "^6.1.0",
        "ejs": "^2.6.1",
        "express": "^4.16.3",
        "express-jwt": "^5.3.1",
        "helmet": "^3.12.0",
        "jsonwebtoken": "^8.3.0",
        "node-dev": "^3.1.3",
        "node-fetch": "^2.1.1",
        "nodemon": "^1.18.7",
